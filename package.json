{
  "name": "vue-csv-loader",
  "version": "0.0.6",
  "private": false,
  "description": "Vue component to load CSV",
  "author": "jacoyutorius",
  "license": "MIT",
  "main": "dist/csv-loader.umd.js",
  "module": "dist/csv-loader.esm.js",
  "unpkg": "dist/csv-loader.min.js",
  "browser": {
    "./sfc": "src/csv-loader.vue"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacoyutorius/vue-csv-loader"
  },
  "keywords": [
    "vue",
    "vuejs",
    "csv",
    "components"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name csv-loader ./src/main.js"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "core-js": "^2.6.5",
    "encoding-japanese": "^1.0.29",
    "vue": "^2.6.10"
  },
  "peerDependencies": {
    "bulma": "^0.7.5",
    "core-js": "^2.6.5",
    "encoding-japanese": "^1.0.29",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
